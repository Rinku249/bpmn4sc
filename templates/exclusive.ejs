template <%= parent.id %> 
    with
        <%= parent.documentation %>
    where
        signatory <%= parent.signatory %>
<% children.forEach((child,index) => { -%>
    <% if (child.length === 1) { %>
        choice <%= parent.name.replaceAll(" ","") %> : ()
            controller <%= parent.signatory %>
            do
                return ()
            <% "\r" -%>
    <% } else {%>
        choice Reject : ContractId <%= child[0].id %>
        <% if (!child[3]) { -%>
    with
            <% child[2].forEach((child2,index2) => { -%>
    <%-child2%>  
            <% }) %><% } -%>
controller <%= parent.signatory %>
            do
                create <%- child[0].id -%> with
                    <% child[1].forEach((child2,index2) => { -%>
<%-child2.split(" :")[0] %> = <%-child2.split(" :")[0] -%> 
                    <% }) -%><% } -%><% })-%> 
